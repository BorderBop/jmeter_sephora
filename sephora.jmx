    default: return 3; // 50% Search Products by Keyword

  }
}
String foo1() {
  int random = Math.floor(Math.random() * 100) + 1; // 1..100
  switch (true) {
    case random &lt; 31: return &quot;Fragrance&quot; // 30%
    case random &lt; 51: return &quot;Lipstick&quot; // 20%
    case random &lt; 71: return &quot;Color&quot; // 20%
    case random &lt; 81: return &quot;Shampoo&quot; // 10%
    case random &lt; 91: return &quot;Skin&quot; // 10%
    case random &lt; 96: return &quot;Eye&quot; // 5%
    case random &lt; 101: return &quot;Face&quot; // 5%
    default: return &quot;nothing&quot;

  }
}
